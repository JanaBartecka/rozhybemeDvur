
<div class="akce">

  {% for aktivity in collections.aktivityMD | reverse %}  

  <div class=" akce__item {{ aktivity.data.myTags }} ">
    <a class="akce__link" href="{{ aktivity.data.title | slugify }}/">
      <div class="akce__datum">{{ aktivity.data.kdy }} </div> 
      
      <div class="akce__image-holder">
        <img src="{{ aktivity.data.foto }}" class="akce__image" alt="{{ aktivity.data.alt }}">
      </div>
      
      <h3 class="akce__title">{{ aktivity.data.title }}</h3>
    </a>
  </div>

  {% endfor %}

</div>

</div>

